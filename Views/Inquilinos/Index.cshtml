@{
    var lista = ViewData["lista"] as List<Inquilino>;
}

<table class="table table dark">
    <thead>
        <tr>
        <th scope="col">Codigo</th>
        <th scope="col">Nombre</th>
        <th scope="col">Apellido</th>
        <th scope="col">Dni</th>
        <th scope="col">Telefono</th>
        <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in lista){
            <tr>
                <th scope="row">@i.Id</th>
                <td>@i.Nombre</td>
                <td>@i.Apellido</td>
                <td>@i.Dni</td>
                <td>@i.Telefono</td>
                <td>@i.Email</td>
                <td><a asp-action="Borrar" asp-route-id=@i.Id>Borrar</a></td>
                <td><a asp-action="Ver" asp-route-id=@i.Id>Ver</a></td>
                <td><a asp-action="Editar" asp-route-id=@i.Id>Editar</a></td>
            </tr>
        }
    </tbody>

</table>
